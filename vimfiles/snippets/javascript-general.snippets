# IIFE wrapper
snippet iife
	(function() {
	
		${1}
	
	})();
# jQuery IIFE wrapper
snippet jqiife
	(function($) {
	
		${1}
	
	})(jQuery);
# test for null
snippet testexist
	if (typeof ${1:variable} !== "undefined" && $1 !== null) {
		${2}
	}
# console.log
snippet cl
	console.log(${1});
# require
snippet rq
	var ${1:variable} = require('${2:module}');${3}
# BlueBird Promises
snippet newpromise
	new Promise(function(resolve, reject) {})
